    "version": "0.2.0",
    "configurations": [
        {
            "name": "flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "web.py",
                "FLASK_ENV": "development"
            },
            "args": [
                "run",
                "--no-debugger"
            ],
            "jinja": true
        },
        {
            "name": "grid",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "console": "integratedTerminal"
        },
    ]
}